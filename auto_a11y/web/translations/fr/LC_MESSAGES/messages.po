msgid "Navigation landmark uses generic term \"navigation\" in label"
msgstr ""

#. Issue: WarnNavLandmarkAccessibleNameUsesNavigation, Field: what
msgctxt "issue_WarnNavLandmarkAccessibleNameUsesNavigation_what"
msgid "Navigation landmark uses generic term \"navigation\" in label"
msgstr ""

#. Issue: WarnNavLandmarkAccessibleNameUsesNavigation, Field: why
msgctxt "issue_WarnNavLandmarkAccessibleNameUsesNavigation_why"
msgid "Generic labels don't describe specific purpose"
msgstr ""

#. Issue: WarnNavLandmarkAccessibleNameUsesNavigation, Field: who
msgctxt "issue_WarnNavLandmarkAccessibleNameUsesNavigation_who"
msgid "Screen reader users"
msgstr ""

#. Issue: WarnNavLandmarkAccessibleNameUsesNavigation, Field: remediation
msgctxt "issue_WarnNavLandmarkAccessibleNameUsesNavigation_remediation"
msgid ""
"Use more descriptive labels like \"Product categories\" or \"User account "
"menu\""
msgstr ""

#. Issue: WarnNavLandmarkHasNoLabel, Field: title
msgctxt "issue_WarnNavLandmarkHasNoLabel_title"
